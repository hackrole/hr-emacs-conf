#!/usr/bin/env python
# encoding: utf-8

import ConfigParser
from evernote.api.client import EvernoteClient
from settings import TOKEN


def shownote():
    client = EvernoteClient(token=TOKEN)
    noteStore = client.get_note_store()

    notelist  =  noteStore.findNotes()
    print notelist


if __name__ == '__main__':
    shownote()
